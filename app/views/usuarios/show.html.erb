<%- model_class = Usuario -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-vertical">
  <dt><strong><%= model_class.human_attribute_name(:First) %>:</strong></dt>
  <dd><%= @usuario.First %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Name) %>:</strong></dt>
  <dd><%= @usuario.Name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Last) %>:</strong></dt>
  <dd><%= @usuario.Last %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Email) %>:</strong></dt>
  <dd><%= @usuario.Email %></dd>
  <dt><strong><%= model_class.human_attribute_name(:CPF) %>:</strong></dt>
  <dd><%= @usuario.CPF %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Telefone) %>:</strong></dt>
  <dd><%= @usuario.Telefone %></dd>
  <dt><strong><%= model_class.human_attribute_name(:CEP) %>:</strong></dt>
  <dd><%= @usuario.CEP %></dd>
  <dt><strong><%= model_class.human_attribute_name(:RuaAv) %>:</strong></dt>
  <dd><%= @usuario.RuaAv %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Numero) %>:</strong></dt>
  <dd><%= @usuario.Numero %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Complemento) %>:</strong></dt>
  <dd><%= @usuario.Complemento %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Bairro) %>:</strong></dt>
  <dd><%= @usuario.Bairro %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Cidade) %>:</strong></dt>
  <dd><%= @usuario.Cidade %></dd>
  <dt><strong><%= model_class.human_attribute_name(:UF) %>:</strong></dt>
  <dd><%= @usuario.UF %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Pais) %>:</strong></dt>
  <dd><%= @usuario.Pais %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              usuarios_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_usuario_path(@usuario), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              usuario_path(@usuario),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
